{
  "name": "lab-dev-extreme-archetype",
  "version": "0.0.1",
  "engines": {
    "node": "= 12.13.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node --max_old_space_size=4098 ./node_modules/@angular/cli/bin/ng build --prod --base-href /lab-dev-extreme-archetype/ --deploy-url /lab-dev-extreme-archetype/ --verbose=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-themes": "devextreme build",
    "tslint": "npx tslint -c tslint.json 'src/**/*{.ts,.tsx}'",
    "tslint:fix": "npx tslint --fix -c tslint.json 'src/**/*{.ts,.tsx}'",
    "tslint:check": "npx tslint-config-prettier-check ./tslint.json",
    "prettier": "npx prettier --check 'src/**/*.ts'",
    "prettier:fix": "npx prettier --write 'src/**/*.ts'",
    "sass-lint": "npx sass-lint -c .sass-lint.yml 'src/**/*.scss' -v -q",
    "sass-lint:fix": "npx sass-lint-auto-fix -c .sass-lint.yml",
    "lint:all": "npx npm-run-all tslint:check tslint sass-lint",
    "fix:all": "npx npm-run-all tslint:fix sass-lint:fix",
    "postinstall": "npm run build-themes"
  },
  "private": true,
  "cldr-data-urls-json": "../../cldrdatadwnl.json",
  "dependencies": {
    "@angular/animations": "11.2.14",
    "@angular/cdk": "8.2.3",
    "@angular/common": "11.2.14",
    "@angular/compiler": "11.2.14",
    "@angular/core": "11.2.14",
    "@angular/flex-layout": "11.0.0-beta.33",
    "@angular/forms": "11.2.14",
    "@angular/platform-browser": "11.2.14",
    "@angular/platform-browser-dynamic": "11.2.14",
    "@angular/router": "11.2.14",
    "@fortawesome/angular-fontawesome": "0.9.0",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@code-art/angular-globalize": "3.2.0",
    "cldr": "4.14.0",
    "cldr-data": "34.0.0",
    "devextreme": "20.2.7",
    "devextreme-angular": "20.2.7",
    "devextreme-schematics": "1.2.14",
    "jszip": "3.6.0",
    "rxjs": "6.6.7",
    "tslib": "2.2.0",
    "zone.js": "0.11.4",
    "keycloak-angular": "^8.1.0",
    "keycloak-js": "^12.0.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.13",
    "@angular/cli": "11.2.13",
    "@angular/compiler-cli": "11.2.14",
    "@types/globalize": "0.0.37",
    "@types/jasmine": "3.7.4",
    "@types/node": "12.20.13",
    "codelyzer": "6.0.2",
    "devextreme-cldr-data": "1.0.3",
    "devextreme-cli": "1.2.14",
    "globalize": "1.6.0",
    "devextreme-themebuilder": "20.2.7",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.6.0",
    "protractor": "7.0.0",
    "ts-node": "8.10.2",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.1.5"
  }
}
